{
  "name": "express-api-starter-ts",
  "version": "1.2.0",
  "description": " A basic starter for an express.js API with Typescript",
  "main": "src/index.ts",
  "scripts": {
    "dev": "run-p build:watch start:watch",
    "build": "swc src -d dist --copy-files",
    "build:watch": "swc src -d dist --copy-files --watch",
    "start": "dotenv -- node dist/index.js",
    "start:watch": "dotenv -- nodemon dist/index.js",
    "gen:model": "npx prisma generate",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "yarn gen:model"
  },
  "keywords": [],
  "author": "CJ R. <cj@null.computer> (https://w3cj.now.sh)",
  "repository": {
    "type": "git",
    "url": "https://github.com/w3cj/express-api-starter.git"
  },
  "license": "MIT",
  "dependencies": {
    "@mysten/kiosk": "^0.8.3",
    "@mysten/sui.js": "^0.50.1",
    "@prisma/client": "5.10.2",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "viem": "^2.7.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prisma": "5.10.2",
    "supertest": "^6.3.3",
    "swc": "^1.0.11",
