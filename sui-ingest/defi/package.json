    "@mysten/sui.js": "^0.49.1",
    "@prisma/client": "5.9.1",
    "@scallop-io/sui-scallop-sdk": "^0.44.18",
    "aftermath-ts-sdk": "^1.1.42",
    "athena-struct-parser": "^1.1.0",
    "axios": "^1.6.7",
    "bn.js": "^5.2.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.1",
    "express": "^4.19.1",
    "fast-csv": "^5.0.1",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "kafkajs-snappy": "^1.1.0",
