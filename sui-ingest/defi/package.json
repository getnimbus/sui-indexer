    "uuid": "^9.0.1",
    "viem": "^2.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.8",
    "dotenv-cli": "^7.3.0",
    "npm-run-all": "^4.1.5",
    "prisma": "5.9.1",
    "swc": "^1.0.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
